class Car:
    def __init__(self, x, y, speed):
        self.location = PVector(x, y)
        self.speed = speed
    
    def move(self):
        self.location.x += self.speed

    def draw(self):
        fill(255)
        rect(self.location.x, self.location.y, 60, 40)
        if self.location.x > width:
            self.location.x = 10
